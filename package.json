{
  "name": "webpack-starter-kit",
  "version": "1.0.0",
  "description": "kit de inicio para trabajar con webpack y vanilla Y sass",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "dev2": "webpack --mode development ./prueba/dev/main.js --output ./prueba/public/script.js",
    "build2": "webpack --mode production ./prueba/dev/main.js --output ./prueba/public/script.js",
    "dev-babel": "webpack --mode development --modulo-bind js=babel-loader",
    "build-babel": "webpack --mode production --modulo-bind js=babel-loader"
  },
  "author": "Valeria Torres <torresvalemdz@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  }
}
